\newcommand*{\iso}{\mathrel{\mathpalette\@iso{}}}
\newcommand*{\@iso}[2]{
  % Calculate the amount of moving \sim up as in \simeq
  \sbox0{$#1\simeq$}
  \sbox2{$#1\sim$}
  \dimen@=\ht0
  \advance\dimen@ by -\ht2
  \sbox0{\lower1.9\dimen@\hbox{$\m@th#1\relbar\iso@joinrel\relbar$}}
  \rlap{\hbox to \wd0{\hfill\raise\dimen@\hbox{$\m@th#1\sim$}\hfill}}
  \copy0
}
\newcommand*{\iso@joinrel}{
  \mathrel{
    \mkern-3.4mu
    \mkern-1mu
    \nonscript\mkern1mu
  }
}
